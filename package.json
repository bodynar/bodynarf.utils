{
    "name": "@bodynarf/utils",
    "version": "1.7.0",
    "author": {
        "name": "Artem",
        "email": "bodynar@gmail.com"
    },
    "license": "MIT",
    "repository": {
        "url": "https://github.com/bodynar/bodynarf.utils",
        "type": "git"
    },
    "bugs": {
        "url": "https://github.com/bodynar/bodynarf.utils/issues",
        "email": "bodynar@gmail.com"
    },
    "keywords": [
        "js",
        "javascript",
        "ts",
        "typescript",
        "function",
        "common",
        "utilities"
    ],
    "scripts": {
        "build": "npx tsc",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "copy_main": "copyfiles package.json readme.md .npmignore dist",
        "transfer": "npm run copy_main",
        "build_transfer": "npm run build && npm run transfer",
        "prepublish": "npm run test:run && npm run build_transfer && cd dist && npm pack",
        "publish_pkg": "npm run test:run && npm run build_transfer && cd dist && npm publish --access=public"
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^0.34.0",
        "copyfiles": "^2.4.1",
        "jsdom": "^26.1.0",
        "typescript": "^5.0.2",
        "vitepress": "^1.6.4",
        "vitest": "^0.34.0"
    }
}
